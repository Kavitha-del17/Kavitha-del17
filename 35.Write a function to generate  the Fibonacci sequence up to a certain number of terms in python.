def generate_fibonacci(n):
    fibonacci_sequence = []
    a, b = 0, 1
    count = 0
    
    while count < n:
        fibonacci_sequence.append(a)
        a, b = b, a + b
        count += 1
    
    return fibonacci_sequence

# Example usage:
num_terms = 10
fibonacci_sequence = generate_fibonacci(num_terms)
print(f"Fibonacci sequence up to {num_terms} terms:")
print(fibonacci_sequence)
